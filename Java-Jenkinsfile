pipeline {
    agent any
    environment {
        AWS_REGION='us-east-2'
    }
    tools{
      maven "Maven"
    }
    stages {
        stage('git') {
            steps {
                git branch: 'dev', url: 'https://github.com/QuantumQuackDoctor/order-service.git'
            }
        }
        stage('test') {
            steps {
                sh "mvn clean test"
            }
        }
}
}
